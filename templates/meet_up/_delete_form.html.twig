<form method="post" action="{{ path('meet_up_delete', {'id': meet_up.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ meet_up.id) }}">
    <button class="btn">Delete</button>
</form>
